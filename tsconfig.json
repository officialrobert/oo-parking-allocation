{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react-jsx",
    "module": "commonjs",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ES6", "DOM"],
    "types": ["react", "react-dom"]
  },
  "exclude": [
    "dist",
    "public",
    "build",
    "client",
    "webpack.config.js",
    "node_modules"
  ]
}
